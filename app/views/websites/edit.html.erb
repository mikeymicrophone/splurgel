<h1> website</h1>

<% form_for(@website) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :href %><br />
    <%= f.text_field :href %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit :repair %>
  </p>
<% end %>

<%= link_to 'show', @website %> |
<%= link_to 'websites', websites_path %>

