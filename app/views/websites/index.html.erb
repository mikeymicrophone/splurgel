<h1>websites</h1>

<table>
<% for website in @websites %>
  <tr>
    <td><%= link_to website.name, website.href %></td>
    <td><%= link_to_name website.user %></td>
    <td><%= link_to 'show', website %></td>
    <td><%= link_to 'edit', edit_website_path(website) %></td>
    <td><%= attach_image_link website %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'new website', new_website_path %>
