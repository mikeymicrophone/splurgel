<h1>Listing phone_uses</h1>

<table>
  <tr>
  </tr>

<% for phone_use in @phone_uses %>
  <tr>
    <td><%= phone_use.phone.number %></td>
    <td><%= link_to_name phone_use.target %></td>
    <td><%= link_to 'Show', phone_use %></td>
    <td><%= link_to 'Edit', edit_phone_use_path(phone_use) %></td>
  </tr>
<% end %>
</table>

