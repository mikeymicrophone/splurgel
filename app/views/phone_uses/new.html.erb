<h1>New phone_use</h1>

<% form_for(@phone_use) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.select :phone_id, Phone.map_name_and_id('number') %>
    <%= f.select :target_type, PhoneUse.targets %>
    <%= f.text_field :target_id %>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', phone_uses_path %>
