<h1> website_use</h1>

<% form_for(@website_use) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :target_type %><br />
    <%= f.select :target_type, ['Brand', 'City', 'Comment', 'Group', 'Image', 'Location', 'Message', 'Network', 'Offering', 'Product', 'State', 'Store', 'Tagging', 'User'] %>
  </p>
  <p>
    <%= f.label :target_id %><br />
    <%= f.text_field :target_id %>
  </p>
  <p>
    <%= f.label :website_id %><br />
    <%= f.text_field :website_id, Website.map_name_and_id %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'website_uses', website_uses_path %>

