<h1>brands</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Phone</th>
  </tr>

<% for brand in @brands %>
  <tr>
    <td><%= link_to_name brand %></td>
    <td><%=h brand.phone %></td>
    <td><%= attach_image_link brand %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'new brand', new_brand_path %>
