<h1>New blog_post</h1>

<% form_for(@blog_post) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :blog_id %><br />
    <%= f.text_field :blog_id %>
  </p>
  <p>
    <%= f.label :post_id %><br />
    <%= f.text_field :post_id %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', blog_posts_path %>
