<h1> network</h1>

<% form_for(@network) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.text_field :name %>    <%= f.label :name, 'network name' %>
  </p>
  <p>
    <%= f.select :city_id, City.map_name_and_id, :include_blank => true %>    <%= f.label :city_id, 'city' %>
		<%= link_to 'another city', new_city_path %>
  </p>
  <p>
    <%= f.select :state_id, State.map_name_and_id, :include_blank => true %>    <%= f.label :state_id, 'state' %>
		<%= link_to 'another state', new_state_path %>
  </p>
  <p>
    <%= f.submit :special %>
  </p>
<% end %>

<%= link_to 'networks', networks_path %>
<%= link_to_backup %>
