<h1> network</h1>

<% form_for(@network) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :city_id %><br />
    <%= f.select :city_id, City.map_name_and_id %>
  </p>
  <p>
    <%= f.label :state_id %><br />
    <%= f.text_field :state_id, State.map_name_and_id %>
  </p>
  <p>
    <%= f.submit :special %>
  </p>
<% end %>

<%= link_to 'networks', networks_path %>
<%= link_to_backup %>
