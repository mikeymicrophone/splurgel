<h1>New address_use</h1>

<% form_for(@address_use) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :target_type %><br />
    <%= f.text_field :target_type %>
  </p>
  <p>
    <%= f.label :target_id %><br />
    <%= f.text_field :target_id %>
  </p>
  <p>
    <%= f.label :address_id %><br />
    <%= f.text_field :address_id, Address.map_name_and_id %>
  </p>
  <p>
    <%= f.submit :envision %>
  </p>
<% end %>

<%= link_to 'address_uses', address_uses_path %>
<%= link_to_backup %>
