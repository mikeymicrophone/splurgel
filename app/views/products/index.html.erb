<h1>products</h1>

<table>
<% for product in @products %>
  <tr>
    <td><%= link_to_name product %></td>
    <td><%= link_to_name product.brand %></td>
    <td><%= link_to 'edit', edit_product_path(product) %></td>
    <td><%= attach_image_link product %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'new product', new_product_path %>
