<h1>New store</h1>

<% form_for(@store) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :administrator_id %><br />
    <%= f.text_field :administrator_id %>
  </p>
  <p>
    <%= f.label :primary_image_id %><br />
    <%= f.text_field :primary_image_id %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', stores_path %>
