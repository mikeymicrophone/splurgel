<h1> address</h1>

<% form_for(@address) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :alternate_names %><br />
    <%= f.text_field :alternate_names %>
  </p>
  <p>
    <%= f.label :street %><br />
    <%= f.text_field :street %>
  </p>
  <p>
    <%= f.label :street2 %><br />
    <%= f.text_field :street2 %>
  </p>
  <p>
    <%= f.label :city_id %><br />
    <%= f.select :city_id, City.map_name_and_id %>
    <%= link_to 'new city', new_city_path %>
  </p>
  <p>
    <%= f.label :state_id %><br />
    <%= f.select :state_id, State.map_name_and_id %>
    <%= link_to 'new state', new_state_path %>
  </p>
  <p>
    <%= f.label :zip %><br />
    <%= f.text_field :zip %>
  </p>
  <p>
    <%= f.submit :encode %>
  </p>
<% end %>

<%= link_to 'addresses', addresses_path %>
<%= link_to_backup %>
